        "@types/node": "^12.11.1",
        "@types/rollup": "^0.54.0",
        "@types/standard-version": "^7.0.0",
        "@typescript-eslint/eslint-plugin": "4.3.0",
        "@typescript-eslint/parser": "4.3.0",
        "codelyzer": "^6.0.0",
        "commitizen": "^4.2.1",
        "concurrently": "^5.3.0",
